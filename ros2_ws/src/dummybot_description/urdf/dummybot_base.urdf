  </joint>

  <!-- LiDAR D500 Base Link (pentru compatibilitate cu driver) -->
  <link name="ldlidar_base"/>

  <joint name="lidar_base_joint" type="fixed">
    <parent link="base_link"/>
    <child link="ldlidar_base"/>
    <origin xyz="0.1167 0 0.177" rpy="0 0 0"/>
  </joint>

  <!-- LiDAR D500 Link (senzorul efectiv) -->
  <link name="ldlidar_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.01929" length="0.0333"/>
      </geometry>
      <material name="black"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.01929" length="0.0333"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.2"/>
      <inertia ixx="0.00002" ixy="0.0" ixz="0.0" 
               iyy="0.00002" iyz="0.0" 
               izz="0.00004"/>
    </inertial>
  </link>

  <joint name="lidar_joint" type="fixed">
    <parent link="ldlidar_base"/>
    <child link="ldlidar_link"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </joint>

</robot>