<?xml version="1.0"?>
<robot name="dummybot">

  <!-- Material definitions -->
  <material name="blue">
    <color rgba="0.2 0.2 0.8 1.0"/>
  </material>
  
  <material name="black">
    <color rgba="0.1 0.1 0.1 1.0"/>
  </material>
  
  <material name="gray">
    <color rgba="0.5 0.5 0.5 1.0"/>
  </material>

  <!-- Base footprint - reference frame on ground -->
  <link name="base_footprint"/>

  <!-- Base link - main chassis/carcasa -->
  <link name="base_link">
    <visual>
      <origin xyz="0 0 0.076" rpy="0 0 0"/>
      <geometry>
        <box size="0.4685 0.384 0.152"/>
      </geometry>
      <material name="blue"/>
    </visual>
    <collision>
      <origin xyz="0 0 0.076" rpy="0 0 0"/>
      <geometry>
        <box size="0.4685 0.384 0.152"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0.076" rpy="0 0 0"/>
      <mass value="5.0"/>
      <inertia ixx="0.1015" ixy="0.0" ixz="0.0" 
               iyy="0.1166" iyz="0.0" 
               izz="0.2068"/>
    </inertial>
  </link>

  <joint name="base_footprint_joint" type="fixed">
    <parent link="base_footprint"/>
    <child link="base_link"/>
    <origin xyz="0 0 0.0" rpy="0 0 0"/>
  </joint>

  <!-- Motor mount + motor assemblies (4x) -->
  
  <!-- Front Left Motor Mount -->
  <link name="motor_mount_front_left">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.04934 0.0398 0.046"/>
      </geometry>
      <material name="gray"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.04934 0.0398 0.046"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.3"/>
      <inertia ixx="0.000062" ixy="0.0" ixz="0.0"
               iyy="0.000071" iyz="0.0"
               izz="0.000038"/>
    </inertial>
  </link>

  <joint name="motor_mount_front_left_joint" type="fixed">
    <parent link="base_link"/>
    <child link="motor_mount_front_left"/>
    <origin xyz="0.17558 0.1721 -0.023" rpy="0 0 0"/>
  </joint>

  <!-- Front Right Motor Mount -->
  <link name="motor_mount_front_right">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.04934 0.0398 0.046"/>
      </geometry>
      <material name="gray"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.04934 0.0398 0.046"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.3"/>
      <inertia ixx="0.000062" ixy="0.0" ixz="0.0"
               iyy="0.000071" iyz="0.0"
               izz="0.000038"/>
    </inertial>
  </link>

  <joint name="motor_mount_front_right_joint" type="fixed">
    <parent link="base_link"/>
    <child link="motor_mount_front_right"/>
    <origin xyz="0.17558 -0.1721 -0.023" rpy="0 0 0"/>
  </joint>

  <!-- Rear Left Motor Mount -->
  <link name="motor_mount_rear_left">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.04934 0.0398 0.046"/>
