      <material name="gray"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.04934 0.0398 0.046"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.3"/>
      <inertia ixx="0.000062" ixy="0.0" ixz="0.0"
               iyy="0.000071" iyz="0.0"
               izz="0.000038"/>
    </inertial>
  </link>

  <joint name="motor_mount_rear_left_joint" type="fixed">
    <parent link="base_link"/>
    <child link="motor_mount_rear_left"/>
    <origin xyz="-0.20158 0.1721 -0.023" rpy="0 0 0"/>
  </joint>

  <!-- Rear Right Motor Mount -->
  <link name="motor_mount_rear_right">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.04934 0.0398 0.046"/>
      </geometry>
      <material name="gray"/>
    </visual>
